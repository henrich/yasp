#!/bin/sh
# postinst script for failinst

set -e

case "$1" in
    configure)
    # try to non-exist directory "foobar", and it's directory, so "rm" cannot
    # remove direcotry without certain option
    rm /foobar
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
